<?xml version="1.0"?>
<launch>
  <env name="WEBOTS_HOME" value="/usr/local/webots"/>

  <arg name="no_gui" default="false," doc="Start Webots with minimal GUI"/>
  <include file="$(find webots_ros)/launch/webots.launch">
    <arg name="mode" value="pause"/>
    <arg name="no_gui" value="$(arg no_gui)"/>
    <arg name="world" value="$(find bring_up_web)/worlds/basic_highway.wbt"/>
  </include>
  
  
  <node name="rviz" pkg="rviz" type="rviz" respawn="false" output="screen" args="-d $(find bring_up_web)/rviz/basic_car.rviz"/>
</launch>
